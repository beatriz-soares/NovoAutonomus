{
  "name": "Auton",
  "version": "1.0.0",
  "description": "Primeira versão da área de trabalho do Autonumus",
  "main": "main.js",
  "scripts": {
    "install": "./node_modules/.bin/electron-rebuild -$(electron -v)",
    "start": "electron main.js",
    "win32": "electron-packager . AutonomusGUI --platform=win32 --arch=ia32",
    "win64": "electron-packager . AutonomusGUI --platform=win32 --arch=x64",
    "linux32": "electron-packager . AutonomusGUI --platform=linux --arch=ia32",
    "linux64": "electron-packager . AutonomusGUI --platform=linux --arch=x64",
    "osx": "electron-packager . AutonomusGUI --platform=darwin --arch=x64",
    "build": "electron-packager . --all"
  },
  "devDependencies": {
    "angular-utils-pagination": "^0.11.0",
    "electron-packager": "^6.0.0",
    "electron-prebuilt": "^0.36.12",
    "electron-rebuild": "~1.4.0",
    "jquery": "^2.2.2",
    "node-gyp": "^3.3.1",
    "node-pre-gyp": "^0.6.28",
    "sqlite-sync": "^0.2.1"
  },
  "dependencies": {
    "angular": "^1.6.4",
    "angular-animate": "^1.6.4",
    "angular-aria": "~1.5.9",
    "angular-carousel": "^1.0.1",
    "angular-material": "^1.1.3",
    "angular-route": "^1.6.4",
    "angular-timer": "^1.3.3",
    "angular-touch": "^1.6.4",
    "electron-debug": "^1.1.0",
    "electron-window-manager": "~1.0.1",
    "events": "~1.1.1",
    "serialport": "^3.1.2",
    "watchjs": "0.0.0"
  }
}
