{
  "name": "Auton",
  "version": "1.0.0",
  "description": "Primeira versão da área de trabalho do Autonumus",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "configure": "npm rebuild speaker --runtime=electron --target=$(electron -v)",
    "win32": "electron-packager . AutonomusGUI --platform=win32 --arch=ia32",
    "win64": "electron-packager . AutonomusGUI --platform=win32 --arch=x64",
    "linux32": "electron-packager . AutonomusGUI --platform=linux --arch=ia32",
    "linux64": "electron-packager . AutonomusGUI --platform=linux --arch=x64",
    "osx": "electron-packager . AutonomusGUI --platform=darwin --arch=x64",
    "build": "electron-packager . --all"
  },
  "devDependencies": {
    "angular-utils-pagination": "^0.11.0",
    "electron": "1.6.10",
    "electron-packager": "^8.7.0",
    "electron-rebuild": "^1.5.11",
    "node-gyp": "^3.3.1",
    "node-pre-gyp": "^0.6.28"
  },
  "dependencies": {
    "angular": "^1.6.4",
    "angular-animate": "^1.6.4",
    "angular-aria": "~1.5.9",
    "angular-route": "^1.6.4",
    "angular-timer": "^1.3.3",
    "angular-touch": "^1.6.4",
    "events": "~1.1.1",
    "jquery": "^3.2.1",
    "pupil-remote": "^0.1.0",
    "speaker": "^0.3.0",
    "sqlite-sync": "^0.3.6",
    "watchjs": "0.0.0",
    "wav": "^1.0.1",
    "xmlbuilder": "^9.0.4"
  }
}
